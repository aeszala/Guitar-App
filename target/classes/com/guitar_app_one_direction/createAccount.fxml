      <VBox layoutX="6.0" layoutY="268.0" prefHeight="200.0" prefWidth="383.0">
         <children>
            <TextField fx:id="txt_email" promptText="Email" />
            <TextField fx:id="txt_username" promptText="Username" />
            <TextField fx:id="txt_password" promptText="Password" />
            <TextField fx:id="txt_password2" promptText="Confirm Password" />
         </children>
      </VBox>
      <Button fx:id="signUpButton" layoutX="113.0" layoutY="475.0" mnemonicParsing="false" onAction="#signUpClicked" onMouseClicked="#btnLoginClicked" prefHeight="24.0" prefWidth="173.0" text="Sign Up" />
      <Button layoutX="14.0" layoutY="860.0" mnemonicParsing="false" onAction="#goToPrimary" text="&lt;" />
      <Label layoutX="143.0" layoutY="157.0" text="Melodex">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
